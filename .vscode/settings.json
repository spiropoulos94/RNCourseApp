        "**/releases/**",
        "**/static/**",
        "**/target/**",
        "**/third_party/**",
        "**/vendor/**",
        "**/ace/**"
    ],
    "material-icon-theme.folders.associations": {
        "application": "app",
        "third-party": "bower",
        "rsscache": "temp",
        "reducers": "redux-reducer",
