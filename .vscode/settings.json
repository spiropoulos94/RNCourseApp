        "actions": "redux-actions",
        "sagas": "redux-store",
        "staging": "server",
        "kitchensink": "examples",
        "jest": "test",
        "mixins": "middleware",
        "endpoints": "api",
        "directives": "vue-directives",
        "bookmarklet": "middleware",
